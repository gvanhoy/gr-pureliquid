<?xml version="1.0"?>
<block>
  <name>file_source</name>
  <key>pureliquid_file_source</key>
  <category>[pureliquid]</category>
  <import>import pureliquid</import>
  <make>pureliquid.file_source($file_name, $repeat, $period_ms)</make>
 <param>
    <name>File Name</name>
    <key>$file_name</key>
    <type>string</type>
  </param>
 <param>
    <name>Repeat</name>
    <key>$repeat</key>
    <type>bool</type>
  </param>
 <param>
    <name>Repetition Period (ms)</name>
    <key>$period_ms</key>
    <type>int</type>
  </param>
 <source>
    <name>pdu</name>
    <type>message</type>
  </source>
</block>
